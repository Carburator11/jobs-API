<head>
    <!DOCTYPE html>
    <html>
    <head>
    
      <title>Jobs</title>
      <meta http-equiv="Cache-control" content="public">
      <meta name="viewport" content="width=device-width, user-scalable=no">
      <link href='../css/style.css' rel='stylesheet'>
</head>
<body>
           
<h1>Jobs DB</h1>

<h3>Filtered DB (<a href="/">reset</a>)</h3>
<div id= "jobsDB">

    <% jobs.forEach( (elem) => { %>
      <% if(elem.name){ %>  
        <div class="companyDiv">
          <%- JSON.stringify(elem.name).replace(/"/g, '')%>
        </div>
              <% elem.jobs.forEach( (el) => { %>
                  <div class="jobDiv">
                    <%- JSON.stringify(el.position).replace(/"/g, '') %>
                    (<%- JSON.stringify(el.experience).replace(/"/g, '') %>), 
                    salary: <%- JSON.stringify(el.salary).replace(/"/g, '') %> kâ‚¬
                    [<a href= "job-<%- el._id %>" </a>link</a>]
                  </div>
              <% })  %>

        
        
        <% }  %>
      
    <% }) %>
    </div>

    


    </div>
</body>



